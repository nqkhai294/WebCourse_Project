{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    },
    "content_format": {
      "type": "enumeration",
      "enum": [
        "md",
        "html"
      ],
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "release"
      ],
      "default": "draft",
      "required": true
    }
  }
}
