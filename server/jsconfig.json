{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowJs": true,
      "checkJs": false,
      "baseUrl": "./",
      "paths": {
          "*": ["node_modules/*", "src/types/*"]
      }
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "**/node_modules/*"
  ]
}
